<% if @policy_plan.strategy %>
  document.getElementById('goal-game-<%= @goal.id %>').innerHTML = "<%= j render 'policy_plans/partials/goal_game_edit_pp', policy_plan: @policy_plan, goal: @goal, game_question: @game_question, new_game_question: nil, game_answers: @game_answers, new_game_answer: @new_game_answer %>";
<% else %>
  document.getElementById('game_questions').innerHTML = "<%= j render 'policy_plans/partials/gamebook_edit_pp', game_questions: @game_questions, game_answers: @game_answers, new_game_answer: @new_game_answer %>";
  document.getElementById('new_game_question').innerHTML = "<%= j render 'policy_plans/partials/new_game_question', policy_plan: @policy_plan, new_game_question: @new_game_question %>";
<% end %>
